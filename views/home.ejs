<%-include("partials/header.ejs");-%>
<h1>Home</h1>
<p><%=startingContent%></p>
<%posts.forEach(function(post){%>
<h1><%=post.title%></h1>
<p>
    <%if(post.blogPost.length > 100){%>
    <%=post.blogPost.substring(0, 100)+"..."%>
    <a href="posts/<%=post.title%>">Read More</a>
    <%}else{%>
    <%=post.blogPost%>
    <%}%>
</p>

<%});%>
<%-include("partials/footer.ejs");-%>